<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="ClientDetailsEditAddFc" t-inherit="point_of_sale.ClientDetailsEdit" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('client-details-right')]/div[hasclass('client-detail')][5]" position="after">
            <div class='client-detail'>
                <span class='label'>F.C.</span>
                <input class='detail fiscalcode' name='fiscalcode'
                        t-on-change="captureChange" t-att-value='props.partner.fiscalcode || ""'/>
            </div>
        </xpath>
    </t>
</templates>
