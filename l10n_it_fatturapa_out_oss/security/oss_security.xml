<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <!-- Multi - Company Rules -->
    <record id="oss_year_company_rule" model="ir.rule">
        <field name="name">OSS Year multi-company</field>
        <field
            name="model_id"
            ref="model_oss_year"
        />
        <field name="domain_force">
            ['|', ('company_id', '=', False), ('company_id', 'in', user.company_ids.ids)]
        </field>
    </record>

</odoo>
