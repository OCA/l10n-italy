<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="portal_my_delivery_note"
        name="Delivery notes"
        customize_show="True"
        inherit_id="portal.portal_my_home"
        priority="55"
    >
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Delivery Notes</t>
                <t t-set="url" t-value="'/my/delivery-notes'" />
                <t t-set="placeholder_count" t-value="'dn_count'" />
            </t>
        </xpath>
    </template>
</odoo>
