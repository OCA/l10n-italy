<odoo>

    <template id="assets_backend" name="assets" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/css"
                href="/l10n_it_delivery_note/static/src/scss/stock_delivery_note.scss"
            />
        </xpath>
    </template>
    <template id="assets_frontend" name="assets" inherit_id="web.assets_frontend">
        <xpath expr="." position="inside">
            <script
                type="text/javascript"
                src="/l10n_it_delivery_note/static/src/js/stock_delivery_note_portal.js"
            />
        </xpath>
    </template>

</odoo>
